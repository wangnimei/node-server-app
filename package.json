{
  "name": "node-server-app",
  "description": "A node.js server desktop GUI",
  "version": "1.0.0",
  "author": "wangyu <504670645@qq.com>",
  "private": true,
  "main": "main.js",
  "build": {
    "appId": "Terry",
    "asarUnpack": "server",
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg",
      "icon": "src/assets/eva.icns"
    },
    "win": {
      "target": "zip",
      "icon": "src/assets/eva.ico"
    }
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot",
    "watch": "cross-env NODE_ENV=development webpack --watch --hide-modules",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "start": "NODE_ENV=development electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist-win": "electron-builder --win --ia32"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "ejs": "^2.5.7",
    "electron-settings": "^3.1.2",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "serve-favicon": "^2.4.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "electron": "1.8.1",
    "electron-builder": "^19.37.2",
    "electron-rebuild": "^1.6.0",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.4",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "vue": "^2.5.0",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.5.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
